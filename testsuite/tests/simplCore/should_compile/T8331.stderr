
==================== Tidy Core rules ====================
"SPEC useAbstractMonad"
    forall (@ s)
           (irred :: ST @@ s)
           ($dMonadAbstractIOST :: MonadAbstractIOST (ReaderT Int (ST s))).
      useAbstractMonad @ (ReaderT Int (ST s))
                       $dMonadAbstractIOST
                       (irred
                        `cast` (Sub (R:@@s[0] <s>_N) ; (Sub (Sym (R:@@sa[0]
                                                                      <s>_N
                                                                      <Int>_N)) ; Sub (Sym (R:@@rma[0]
                                                                                                <Int>_N
                                                                                                <ST
                                                                                                   s>_N
                                                                                                <Int>_N)))
                                :: Coercible (ST @@ s) (ReaderT Int (ST s) @@ Int)))
      = useAbstractMonad_$suseAbstractMonad @ s irred


